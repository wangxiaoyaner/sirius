global main
extern printf
extern scanf
section .data
strint:'%d',0
strchar:' %c',0
section .text
main:
mov ebp,esp
sub esp,4
mov eax,'b'
push eax
push strchar
call printf
add esp,8
mov eax,'y'
push eax
push strchar
call printf
add esp,8
mov eax,'z'
push eax
push strchar
call printf
add esp,8
mov esp,ebp
ret
